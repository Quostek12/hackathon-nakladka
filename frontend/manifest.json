{
  "manifest_version": 2,
  "name": "Prawda w sieci",
  "version": "1.0",
  "description": "Zobacz to co moze byc ukryte",
  
  "permissions": [
    "tabs",
    "https://twoje-api.pl/*"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
    "browser_action": {
    "default_title": "Moje rozszerzenie",
    "default_popup": "popup.html"
  }
}
